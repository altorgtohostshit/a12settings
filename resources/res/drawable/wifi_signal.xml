<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item app:state_encrypted="true">
        <layer-list>
            <item>
                <level-list>
                    <item android:drawable="@drawable/ic_wifi_signal_1" android:maxLevel="0"/>
                    <item android:drawable="@drawable/ic_wifi_signal_2" android:maxLevel="1"/>
                    <item android:drawable="@drawable/ic_wifi_signal_3" android:maxLevel="2"/>
                    <item android:drawable="@drawable/ic_wifi_signal_4" android:maxLevel="3"/>
                </level-list>
            </item>
            <item android:drawable="@drawable/ic_wifi_signal_lock"/>
        </layer-list>
    </item>
    <item app:state_encrypted="false">
        <level-list>
            <item android:drawable="@drawable/ic_wifi_signal_1" android:maxLevel="0"/>
            <item android:drawable="@drawable/ic_wifi_signal_2" android:maxLevel="1"/>
            <item android:drawable="@drawable/ic_wifi_signal_3" android:maxLevel="2"/>
            <item android:drawable="@drawable/ic_wifi_signal_4" android:maxLevel="3"/>
        </level-list>
    </item>
</selector>
