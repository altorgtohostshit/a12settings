<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:key="mobile_network_pref_screen">
    <com.android.settings.widget.SettingsMainSwitchPreference android:key="use_sim_switch" app:controller="com.android.settings.network.telephony.MobileNetworkSwitchController"/>
    <PreferenceCategory android:layout="@layout/preference_category_no_label" android:title="@string/summary_placeholder" android:key="enabled_state_container" app:controller="com.android.settings.network.telephony.DisabledSubscriptionController">
        <com.android.settings.datausage.DataUsageSummaryPreference android:visibility="gone" android:selectable="false" android:key="status_header"/>
        <ListPreference android:title="@string/calls_preference" android:key="calls_preference" app:controller="com.android.settings.network.telephony.CallsDefaultSubscriptionController" app:allowDividerAbove="true"/>
        <ListPreference android:title="@string/sms_preference" android:key="sms_preference" app:controller="com.android.settings.network.telephony.SmsDefaultSubscriptionController"/>
        <Preference android:title="@string/cdma_lte_data_service" android:key="cdma_lte_data_service_key" app:controller="com.android.settings.network.telephony.DataServiceSetupPreferenceController" app:allowDividerAbove="true" app:allowDividerBelow="false"/>
        <SwitchPreference android:title="@string/mobile_data_settings_title" android:key="mobile_data_enable" android:summary="@string/mobile_data_settings_summary" app:controller="com.android.settings.network.telephony.MobileDataPreferenceController" app:allowDividerAbove="true"/>
        <com.android.settingslib.RestrictedSwitchPreference android:persistent="false" android:title="@string/roaming" android:key="button_roaming_key" android:summaryOn="@string/roaming_enable" android:summaryOff="@string/roaming_disable" app:controller="com.android.settings.network.telephony.RoamingPreferenceController" app:userRestriction="no_data_roaming"/>
        <Preference android:title="@string/mobile_data_usage_title" android:key="data_usage_summary" app:controller="com.android.settings.network.telephony.DataUsagePreferenceController"/>
        <com.android.settings.datausage.BillingCyclePreference android:title="@string/billing_cycle" android:key="billing_preference" app:controller="com.android.settings.datausage.BillingCyclePreferenceController"/>
        <SwitchPreference android:title="@string/carrier_wifi_offload_title" android:key="carrier_wifi_toggle" android:summary="@string/carrier_wifi_offload_summary" app:controller="com.android.settings.network.CarrierWifiTogglePreferenceController"/>
        <Preference android:title="@string/carrier_wifi_network_title" android:key="carrier_wifi_network"/>
        <SwitchPreference android:title="@string/mms_message_title" android:key="mms_message" android:summary="@string/mms_message_summary" app:controller="com.android.settings.network.telephony.MmsMessagePreferenceController"/>
        <SwitchPreference android:title="@string/data_during_calls_title" android:key="data_during_calls" android:summary="@string/data_during_calls_summary" app:controller="com.android.settings.network.telephony.DataDuringCallsPreferenceController"/>
        <SwitchPreference android:persistent="false" android:title="@string/enhanced_4g_lte_mode_title" android:key="enhanced_4g_lte" android:summary="@string/enhanced_4g_lte_mode_summary" app:controller="com.android.settings.network.telephony.Enhanced4gLtePreferenceController" app:keywords="@string/keywords_enhance_4g_lte"/>
        <SwitchPreference android:persistent="false" android:title="@string/enhanced_4g_lte_mode_title_4g_calling" android:key="4g_calling" android:summary="@string/enhanced_4g_lte_mode_summary_4g_calling" app:controller="com.android.settings.network.telephony.Enhanced4gCallingPreferenceController" app:keywords="@string/keywords_enhance_4g_lte"/>
        <SwitchPreference android:persistent="false" android:title="@string/enhanced_4g_lte_mode_title_advanced_calling" android:key="advance_call" android:summary="@string/enhanced_4g_lte_mode_summary" app:controller="com.android.settings.network.telephony.Enhanced4gAdvancedCallingPreferenceController" app:keywords="@string/keywords_enhance_4g_lte"/>
        <SwitchPreference android:persistent="false" android:title="@string/contact_discovery_opt_in_title" android:key="contact_discovery_opt_in" android:summary="@string/contact_discovery_opt_in_summary" app:controller="com.android.settings.network.telephony.ContactDiscoveryPreferenceController"/>
        <ListPreference android:entries="@array/preferred_network_mode_choices" android:title="@string/preferred_network_mode_title" android:key="preferred_network_mode_key" android:summary="@string/preferred_network_mode_summary" android:dialogTitle="@string/preferred_network_mode_dialogtitle" android:entryValues="@array/preferred_network_mode_values" app:controller="com.android.settings.network.telephony.PreferredNetworkModePreferenceController"/>
        <ListPreference android:entries="@array/enabled_networks_choices" android:title="@string/preferred_network_mode_title" android:key="enabled_networks_key" android:summary="@string/preferred_network_mode_summary" android:dialogTitle="@string/preferred_network_mode_dialogtitle" android:entryValues="@array/enabled_networks_values" app:controller="com.android.settings.network.telephony.EnabledNetworkModePreferenceController"/>
        <Preference android:enabled="false" android:title="@string/carrier_settings_version" android:selectable="false" android:key="carrier_settings_version_key" android:shouldDisableView="false" app:controller="com.android.settings.network.telephony.CarrierSettingsVersionPreferenceController" app:enableCopying="true"/>
        <PreferenceCategory android:title="@string/call_category" android:key="calling_category" app:controller="com.android.settings.network.telephony.CallingPreferenceCategoryController">
            <PreferenceScreen android:title="@string/wifi_calling_settings_title" android:key="wifi_calling" android:summary="@string/wifi_calling_summary" app:controller="com.android.settings.network.telephony.WifiCallingPreferenceController">
                <intent android:targetPackage="com.android.settings" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings.Settings$WifiCallingSettingsActivity">
                    <extra android:name="show_drawer_menu" android:value="true"/>
                </intent>
            </PreferenceScreen>
            <SwitchPreference android:persistent="true" android:title="@string/video_calling_settings_title" android:key="video_calling_key" app:controller="com.android.settings.network.telephony.VideoCallingPreferenceController"/>
            <SwitchPreference android:persistent="false" android:title="@string/backup_calling_settings_title" android:key="backup_calling_key" android:summary="@string/backup_calling_setting_summary" app:controller="com.android.settings.network.telephony.BackupCallingPreferenceController" app:keywords="@string/keywords_backup_calling"/>
        </PreferenceCategory>
        <com.android.settings.network.telephony.cdma.CdmaListPreference android:entries="@array/cdma_system_select_choices" android:title="@string/cdma_system_select_title" android:key="cdma_system_select_key" android:summary="@string/cdma_system_select_summary" android:dialogTitle="@string/cdma_system_select_dialogtitle" android:entryValues="@array/cdma_system_select_values" app:controller="com.android.settings.network.telephony.cdma.CdmaSystemSelectPreferenceController"/>
        <com.android.settings.network.telephony.cdma.CdmaListPreference android:entries="@array/cdma_subscription_choices" android:title="@string/cdma_subscription_title" android:key="cdma_subscription_key" android:summary="@string/cdma_subscription_summary" android:dialogTitle="@string/cdma_subscription_dialogtitle" android:entryValues="@array/cdma_subscription_values" app:controller="com.android.settings.network.telephony.cdma.CdmaSubscriptionPreferenceController"/>
        <PreferenceCategory android:title="@string/network_operator_category" android:key="network_operators_category_key" app:controller="com.android.settings.network.telephony.NetworkPreferenceCategoryController">
            <SwitchPreference android:title="@string/select_automatically" android:key="auto_select_key" app:controller="com.android.settings.network.telephony.gsm.AutoSelectPreferenceController"/>
            <Preference android:title="@string/choose_network_title" android:key="choose_network_key" android:fragment="com.android.phone.NetworkSelectSetting" app:controller="com.android.settings.network.telephony.gsm.OpenNetworkSelectPagePreferenceController"/>
        </PreferenceCategory>
        <com.android.settingslib.RestrictedPreference android:persistent="false" android:title="@string/mobile_network_apn_title" android:key="telephony_apn_key" app:controller="com.android.settings.network.telephony.ApnPreferenceController" app:allowDividerAbove="true"/>
        <Preference android:title="@string/carrier_settings_title" android:key="carrier_settings_key" app:controller="com.android.settings.network.telephony.CarrierPreferenceController"/>
    </PreferenceCategory>
    <Preference android:persistent="false" android:title="@string/mobile_network_erase_sim" android:key="erase_sim" app:controller="com.android.settings.network.telephony.DeleteSimProfilePreferenceController" app:allowDividerBelow="true"/>
    <com.android.settingslib.widget.FooterPreference android:title="@string/mobile_network_disable_sim_explanation" android:selectable="false" android:key="disable_sim_explanation" app:controller="com.android.settings.network.telephony.DisableSimFooterPreferenceController" app:allowDividerAbove="true" app:searchable="false"/>
    <com.android.settingslib.widget.FooterPreference android:title="@string/no_5g_in_dsds_text" android:selectable="false" android:key="no_5g_in_dsds_explanation" app:controller="com.android.settings.network.telephony.NrDisabledInDsdsFooterPreferenceController" app:allowDividerAbove="true" app:searchable="false"/>
    <SwitchPreference android:title="@string/enable_2g_title" android:key="enable_2g" android:summary="@string/enable_2g_summary" app:controller="com.android.settings.network.telephony.Enable2gPreferenceController"/>
</PreferenceScreen>
